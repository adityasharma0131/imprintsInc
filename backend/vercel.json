{
    "version": 2,
    "builds": [
        {
            "src": "./routes/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "./routes/users.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}